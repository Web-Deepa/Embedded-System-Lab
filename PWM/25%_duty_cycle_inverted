//Generate square wave using fast PWM using inverted mode duty cycle 25%
#include <avr/io.h>
int main(void)
{
    /* Replace with your application code */
	DDRB|=(1<<PB3);
	TCCR0=(1<<WGM00)|(1<<WGM01)|(1<<COM00)|(1<<COM01)|(1<<CS00);
	OCR0=192;//25%  duty cycle ( 255-25% of 255)
    while (1);
   
	return 0;
}
